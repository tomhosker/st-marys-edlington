extends layout

block content
    h1 Service Times

    div.helpbox
        p HELLO, MR DUDLEY!

    table.homepage
        tr
            th Service
            th Weekday
            th Time
            th Location
        for service in serviceTimes
            tr
                td=service.service_type
                td=service.weekdayString
                td=service.time
                td=service.location_name
